{
  "name": "marxist.space-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "postbuild": "rm -rf ../public/static && mv -f build/* ../public/",
    "start": "node scripts/start.js",
    "test": "run-s -s test:ts test:lint test:cover",
    "test:cover": "npm run test:jest -- --coverage --no-cache",
    "test:jest": "node scripts/test.js",
    "test:lint": "eslint -c .eslintrc.json --report-unused-disable-directives --ext .js,.ts,.tsx config scripts src",
    "test:ts": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "babel-core": "7.0.0-bridge.0",
    "fetchyeah": "3.0.8",
    "jest-styled-components": "7.0.0",
    "lodash": "4.17.15",
    "react": "16.12.0",
    "react-app-polyfill": "0.2.1",
    "react-dev-utils": "7.0.3",
    "react-dom": "16.12.0",
    "react-lazyload": "2.6.5",
    "react-markdown": "4.3.1",
    "react-redux": "5.1.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.5.3",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "resolve": "1.15.1",
    "safer-web-storage": "1.1.0",
    "styled-components": "5.0.1",
    "styled-reset": "4.1.0"
  },
  "devDependencies": {
    "@svgr/webpack": "4.1.0",
    "@types/jest": "25.1.3",
    "@types/lodash": "4.14.149",
    "@types/node": "13.7.4",
    "@types/react": "16.9.22",
    "@types/react-dom": "16.9.5",
    "@types/react-lazyload": "2.6.0",
    "@types/react-test-renderer": "16.9.2",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "0.3.6",
    "babel-preset-react-app": "7.0.1",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "eslint": "5.12.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-zacanger": "1.0.2",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "jest": "25.1.0",
    "jest-resolve": "25.1.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-test-renderer": "16.12.0",
    "redux-mock-store": "1.5.4",
    "sortpack": "2.1.1",
    "terser-webpack-plugin": "1.2.2",
    "typescript": "3.8.2",
    "webpack": "4.41.6",
    "webpack-dev-server": "3.10.3",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "3.6.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx",
      "json",
      "jsx"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jest/setup.js"
    ],
    "testEnvironment": "jsdom",
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": "http://server:9090"
}
